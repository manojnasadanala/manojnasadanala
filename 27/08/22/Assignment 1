#include <stdio.h>
#include <string.h>

int main ()
{
   char s1[20]; 
   char s2[20];
   gets(s1);
   gets(s2);
   char temp,count=0,size;
   int i, j;
   int n  = strlen(s1);
   int n1 = strlen(s2);
   size=n;
   for (i = 0; i < n-1; i++)
   {
      for (j = i+1; j < n; j++)
      {
         if (s1[i] > s1[j]) {
            temp  = s1[i];
            s1[i] = s1[j];
            s1[j] = temp;
         }
         if (s2[i] > s2[j])
         {
            temp  = s2[i];
            s2[i] = s2[j];
            s2[j] = temp;
         }
      }
   }
   for(i = 0; i<n; i++)
   {
      if(s1[i] == s2[i])
      {
         count++;
      }
   }
if(count==size)
   printf("ANAGRAMS");
 else
 printf("NOT ANAGRASM");
   return 0;
}

INPUT AND OUTPUT:

below
below
ANAGRAMS
